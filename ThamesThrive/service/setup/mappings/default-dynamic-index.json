{
  "settings": {
    "number_of_shards": %%CONF_SHARDS%%,
    "number_of_replicas": %%REPLICAS%%
  },
  "mappings": {
    "_meta": {
      "version": "%%VERSION%%",
      "name": "%%PREFIX%%"
    },
    "dynamic": "true",
    "properties": {
      "id": {
        "type": "keyword", "ignore_above": 64
      },
      "metadata": {
        "dynamic": "false",
        "properties": {
          "time": {
            "properties": {
              "insert": {
                "type": "date"
              }
            }
          }
        }
      }
    }
  },
  "aliases": {
    "%%ALIAS%%": {}
  }
}
